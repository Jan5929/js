/**index.wxss**/
page{
  width: 100%;
  height: 100%;
}
.header {
  width: 100%;
  height: 508rpx;
  position: relative;
}
.top-bg{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
}
.top-bg image {
  width: 100%;
  height: 432rpx;
}
.project-title{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 1;
}
.logo-img{
  width: 164rpx;
  height: 164rpx;
  position: absolute;
  left: 50%;
  margin-left: -82rpx;
  bottom: 0;
  z-index: 1;
}
.logo-img image {
  width: 100%;
  height: 100%;
}
.but-quick-login{
  width: 690rpx;
  height: 96rpx;
  margin-top: 106rpx;
  border-radius: 10rpx;
  box-shadow:0px 20rpx 20rpx rgba(51,204,153,0.2);
  display: flex;
  flex-flow: row nowrap;
  align-items: center;
  justify-content: center;
  background: #33CC99;
  position: relative;
}
.but-quick-login image {
  width: 48rpx;
  height: 48rpx;
  position: absolute;
  z-index: 1;
  left: 168rpx;
}
.but-quick-login button {
  width: 100%;
  margin: 0;
  background: transparent;
  color: #FFFFFF;
  font-size: 36rpx;
  padding: 0;
  text-align: left;
  padding-left: 228rpx;
}
.but-quick-login button[type=primary] {
  box-shadow: 0px 10px 10px rgba(51, 204, 153, 0.2);
}
.but-quick-login button::after{
  border: none;
}
.but-manual-login button{
  margin-top: 32rpx;
  background: transparent;
  color: #576B95;
  font-size: 36rpx;
}
.but-manual-login button::after {
  border: none;
}
.x-page-footer{
  color: #D6D5D6;
  font-size: 22rpx;
  flex: 1 0 auto;
  display: flex;
  align-items: flex-end;
  justify-content: center;
  padding-bottom: 20rpx;
}